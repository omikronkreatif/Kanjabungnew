<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="action_shipping_dashboard" model="ir.actions.client">
        <field name="name">Shipment Dashboard</field>
        <field name="tag">shipment_dashboard</field>
    </record>
    <template id="asset_transport_management" name="Transport Management" inherit_id="web.assets_backend">
